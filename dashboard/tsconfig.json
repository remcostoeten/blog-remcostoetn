{
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "esModuleInterop": true,
    "downlevelIteration": false,
    "module": "esnext",
    "target": "es2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext.asynciterable"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "noImplicitAny": false,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@c/*": ["./components/*"],
      "@ui/*": ["./components/ui/*"],
      "@core/*": ["./core/*"],
      "@dash/*": ["./app/(dashboard)/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
